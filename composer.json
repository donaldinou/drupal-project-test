{
    "name": "drupal-features/website-standard-edition-test",
    "description": "Drupal Acreat Website",
    "version": "1.0.0",
    "type": "drupal-website",
    "keywords": ["Drupal", "acreat", "website"],
    "homepage": "http://www.acreat.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Régis Hautière",
            "email": "hautiere@acreat.com",
            "homepage": "http://www.acreat.com",
            "role": "Owner"
        },
        {
            "name": "Adrien Loyant",
            "email": "loyant@acreat.com",
            "homepage": "http://www.acreat.com",
            "role": "Developper"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "vcs",
            "url":  "http://gitlab.acreat/composer/drupal-installer.git"
        },
        {
            "type": "vcs",
            "url":  "http://gitlab.acreat/acreat/drupal-acreat-adminimal-admin-toolbar.git"
        },
        {
            "type": "vcs",
            "url":  "http://gitlab.acreat/acreat/drupal-acreat-factory.git"
        },
        {
            "type": "vcs",
            "url":  "http://gitlab.acreat/acreat/drupal-acreat-helper.git"
        },
        {
            "type": "vcs",
            "url":  "http://gitlab.acreat/acreat/drupal-acreat-news.git"
        },
        {
            "type": "vcs",
            "url":  "http://gitlab.acreat/acreat/drupal-theme-abyssin.git"
        }

    ],
    "require": {
        "php": ">=5.5.9",
        "composer/installers": "dev-master",
        "composer/drupal-installer": "dev-master",
        "drupal/admin_toolbar": "~1.3",
        "drupal/config_devel": "~1.0-rc1",
        "drupal/config_update": "~1.2",
        "drupal/ctools": "~3.0-alpha27",
        "drupal/default_content": "^1.0-alpha3",
        "drupal/devel": "~1.0-alpha1",
        "drupal/entity": "~1.0-alpha3",
        "drupal/entity_browser": "~1.0-beta1",
        "drupal/entity_reference_revisions": "~1.0",
        "drupal/features": "~3.0",
        "drupal/google_analytics": "~2.1",
        "drupal/inline_entity_form": "~1.0-beta1",
        "drupal/link_attributes": "~1.0-alpha3",
        "drupal/media_entity": "~1.5",
        "drupal/media_entity_browser": "~1.x-dev",
        "drupal/media_entity_image": "~1.2",
        "drupal/menu_breadcrumb": "~1.0-beta3",
        "drupal/metatag": "~1.0-beta11",
        "drupal/paragraphs": "~1.0",
        "drupal/pathauto": "~1.0-beta1",
        "drupal/search_api": "~1.0-beta3",
        "drupal/simple_sitemap": "~2.7",
        "drupal/token": "~1.0-beta2",
        "drupal/twig_extensions": "~1.0",
        "drupal/yamlform": "~1.0-beta23",
        "drupal-features/acreat_adminimal_admin_toolbar": "dev-master",
        "drupal-features/acreat_factory": "dev-master",
        "drupal-features/acreat_helper": "~8.1.1",
        "drupal-features/acreat_news": "dev-master",
        "drupal-theme/acreat_adminimal_admin_toolbar": "dev-master"
    },
    "conflict": {
        "drupal/drupal": "<8.0 >9.0"
    },
    "support": {
        "email": "support@acreat.com"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "secure-http": false
    },
    "scripts": {
        "post-update-cmd": [
            "echo 'done'"
        ]
    },
    "extra": {
        "drupal-config": "parameters.yml",
        "branch-alias": {
            "dev-master": "8.x-1.x"
        },
        "installer-paths": {
            "libraries/contrib/{$name}/": ["type:drupal-library"],
            "modules/contrib/{$name}/": ["type:drupal-module"],
            "profiles/contrib/{$name}/": ["type:drupal-profile"],
            "themes/contrib/{$name}/": ["type:drupal-theme"]
        }
    }
}

